#Arquivo modelo Vagrantfile

#Versao do Vagrant Utilizado
Vagrant.configure("2") do |config|

#Definindo Imagem(box) para a VM
config.vm.box = "ubuntu/bionic64"
#Definindo provider que sera utilizado e configuracao da VM
  config.vm "virtualbox" do |v|
    v.memory = 1024
    v.cpus = 2
  end
end
#Definindo Variavel para VM Ubuntu
  
